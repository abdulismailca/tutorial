<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_account_view_move_form_inherit_sale_order_ids"
            model="ir.ui.view">
        <field name="name">account.move.form.inherits</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='delivery_date']" position="after">
                <field name="is_existing_invoice_line"/>

                <field name="alternative_sale_order_ids"
                       widget="many2many_tags" domain="[('partner_id','=',partner_id),('state','=','draft')]"/>
            </xpath>
        </field>
    </record>
</odoo>



